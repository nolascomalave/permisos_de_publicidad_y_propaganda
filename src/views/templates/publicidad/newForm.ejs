						<fieldset class="publicidadCantidadList">
							<legend>Tipo de Publicidad: <span class="obligatory">*</span></legend>
							<input type="number" class="cantidad" id="publicidadVolantesNum" min="0" max="100000000" placeholder="Cantidad">
							<label for="publicidadVolantesNum">Volantes</label><br>

							<input type="number" class="cantidad" id="publicidadAfichesNum" min="0" max="100000000" placeholder="Cantidad">
							<label for="publicidadAfichesNum">Afiches</label><br>

							<input type="number" class="cantidad" id="publicidadPendonesNum" min="0" max="100000000" placeholder="Cantidad">
							<label for="publicidadPendonesNum">Pendones</label><br>

							<input type="number" class="cantidad" id="publicidadHabladoresNum" min="0" max="100000000" placeholder="Cantidad">
							<label for="publicidadHabladoresNum">Habladores</label><br>

							<input type="number" class="cantidad" id="publicidadStandsNum" min="0" max="100000000" placeholder="Cantidad">
							<label for="publicidadStandsNum">Stands</label><br>

							<input type="number" class="cantidad" id="publicidadCalcomaniasNum" min="0" max="100000000" placeholder="Cantidad">
							<label for="publicidadCalcomaniasNum">Calcomanías</label><br>

							<input type="number" class="cantidad" id="publicidadBanderolasNum" min="0" max="100000000" placeholder="Cantidad">
							<label for="publicidadBanderolasNum">Banderolas o banderines</label><br>

							<input type="number" class="cantidad" id="publicidadOtrosNum" min="0" max="100000000" placeholder="Cantidad">
							<label for="publicidadOtrosNum">Otros:</label>
							<div style="float: none;" class="onlyDiv">
								<input type="text" id="publicidadOtrosText" placeholder="Otros" minlength="3" maxlength="100">
							</div>
							<p class="error" id="typePublicidadError"></p>
						</fieldset>

						<div class="textarea">
							<label for="comentarios">Comentarios: <span class="obligatory">*</span></label>
							<textarea id="comentarios" required placeholder="Comentarios" maxlength="1000" minlength="10"></textarea>
							<p class="error" id="comentariosError"></p>
						</div>

						<div style="float: none">
							<blockquote id="comprobanteContent">
								<label>Comprobante: <span class="obligatory">*</span></label><br>
								<label for="comprobante" class="file-button">
									<div class="truncado" title="Agregar Comprobante">
										<i class="li-icon fas fa-plus-circle"></i>
										<span>Agregar</span>
										<input type="file" id="comprobante">
									</div>
								</label>
							</blockquote>
							<p class="error" id="comprobanteError"></p>
						</div>

						<div class="cleaner" style="float:none"></div>

						<center>
							<button type="submit" id="processItem">Generar Permiso</button>
						</center>
					</form>
				</section>