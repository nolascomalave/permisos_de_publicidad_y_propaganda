				<header>
					<p>Nuevo Permiso</p>
					<button>
						<img src="/IMG/close.png">
					</button>
				</header>
				<section>
					<form id="itemForm" enctype="multipart/form-data">
						<div class="input-date-continer">
							<label for="habilitacion">Fecha de Habilitación: <span class="obligatory">*</span></label>
							<input type="date" id="habilitacion" required>
							<p class="error" id="habilitacionError"></p>
						</div>

						<div class="input-date-continer">
							<label for="vencimiento">Fecha de Vencimiento: <span class="obligatory">*</span></label>
							<input type="date" id="vencimiento" required>
							<p class="error" id="vencimientoError"></p>
						</div>

						<div>
							<% let hours=gf.hoursDay(), horas=[]; %>
							<label>Horario: <span class="obligatory">*</span></label>
							<br>
							<select id="horarioInicio" required>
								<% hours.forEach((hour)=>{ %>
									<option value="<%- hour %>"><%- hour %></option>
									<% horas.push(hour); %>
								<% }); %>
							</select>
							<span>-</span>
							<select id="horarioFin" required>
								<% horas.forEach((hora)=>{ %>
									<option value="<%- hora %>"><%- hora %></option>
								<% }); %>
							</select>
							<p class="error" id="horarioError"></p>
						</div>

						<div class="checkbox" id="dat-firma-content">
							<label>
								<input type="checkbox" id="dat-firma"> <p>Necesita firma de la DAT</p>
								<span class="checkmark"></span>
							</label>
						</div>

						<span class="cleaner"></span>

						<fieldset >
							<legend>Datos del Requisitor:</legend>
							<div>
								<label for="requisitorNombre">Nombre: <span class="obligatory">*</span></label>
								<input type="text" id="requisitorNombre" required placeholder="Nombre" maxlength="50" minlength="2">
								<p class="error" id="requisitorNombreError"></p>
							</div>
							<div class="margin-left-input">
								<label for="requisitorApellido">Apellido:</label>
								<input type="text" id="requisitorApellido" placeholder="Apellido" maxlength="50" minlength="2">
								<p class="error" id="requisitorApellidoError"></p>
							</div>
							<div class="margin-left-input">
								<label for="requisitorDoc">C.I. o Rif: <span class="obligatory">*</span></label>
								<input type="text" id="requisitorDoc" required placeholder="C.I: o Rif" maxlength="12" minlength="10">
								<p class="error" id="requisitorDocError"></p>
							</div>
							<div>
								<label for="tlfRequisitor">Teléfono: <span class="obligatory">*</span></label>
								<input type="text" id="tlfRequisitor" required placeholder="Teléfono"  maxlength="15" minlength="11">
								<p class="error" id="tlfRequisitorError"></p>
							</div>
							<div class="textarea">
								<label for="habitacionRequisitor">Dirección de Habitación: <span class="obligatory">*</span></label>
								<textarea id="habitacionRequisitor" required placeholder="Dirección" maxlength="1000" minlength="5"></textarea>
								<p class="error" id="habitacionRequisitorError"></p>
							</div>
						</fieldset>

						<div class="textarea">
							<label for="sector">Sector y Área Permisada: <span class="obligatory">*</span></label>
							<textarea id="sector" required placeholder="Sector y Área" maxlength="1000" minlength="5"></textarea>
							<p class="error" id="sectorError"></p>
						</div>

